  "private": true,
  "scripts": {
    "dev": "nodemon src/server.ts",
    "build": "tsc",
    "build:lenient": "tsc --noEmitOnError false --skipLibCheck true",
    "start": "node dist/server.js",
    "lint": "echo 'Linting disabled - ESLint v9 migration needed'",
    "lint:fix": "eslint src --ext .ts --fix",
    "typecheck": "npx tsc --noEmit",
    "migrate": "node dist/database/migrate.js",
    "test": "echo 'Backend tests disabled - @swc/jest configuration needed'",
    "test:unit": "npx jest --testPathPatterns=unit",
    "test:integration": "npx jest --testPathPatterns=integration",
    "test:docker": "ts-node src/tests/docker-test-runner.ts",
    "test:docker:verbose": "ts-node src/tests/docker-test-runner.ts --verbose",
    "test:docker:baseline": "npx jest src/tests/integration/docker-performance-baseline.test.ts --testTimeout=900000",
    "test:docker:integration": "npx jest src/tests/integration/docker-integration.test.ts --testTimeout=600000",
    "test:ci": "npx jest --coverage --ci --watchAll=false --testTimeout=30000",
    "test:watch": "npx jest --watch",
    "test:coverage": "npx jest --coverage --watchAll=false",
    "test:coverage:fix": "npx jest --coverage --watchAll=false --collectCoverageFrom='src/**/*.ts' --collectCoverageFrom='!src/**/*.d.ts' --collectCoverageFrom='!src/tests/**' --collectCoverageFrom='!src/**/*.test.ts' --collectCoverageFrom='!src/**/*.spec.ts' --collectCoverageFrom='!src/types/**'"
  },
